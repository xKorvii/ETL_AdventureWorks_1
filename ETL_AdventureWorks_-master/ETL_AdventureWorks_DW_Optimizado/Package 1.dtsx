                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
    ISNULL(dp.ProductKey, -1)   AS ProductKey,
    ISNULL(dc.CustomerKey, -1)  AS CustomerKey,
    ISNULL(dt.TerritoryKey, -1) AS TerritoryKey,
    ISNULL(dd.DateKey, -1)      AS DateKey,
    ISNULL(dsr.SalesReasonKey, -1) AS SalesReasonKey, 

    -- Datos transaccionales
    soh.SalesOrderNumber,
    sod.OrderQty,
    sod.UnitPrice,
    sod.LineTotal,
    CAST((sod.UnitPrice * sod.OrderQty) AS DECIMAL(18,2)) AS TotalBruto

FROM AdventureWorks2019.Sales.SalesOrderDetail sod
INNER JOIN AdventureWorks2019.Sales.SalesOrderHeader soh 
    ON sod.SalesOrderID = soh.SalesOrderID
LEFT JOIN AdventureDW.dbo.DimProduct dp 
    ON sod.ProductID = dp.ProductID
LEFT JOIN AdventureDW.dbo.DimCustomer dc 
    ON soh.CustomerID = dc.CustomerID
LEFT JOIN AdventureDW.dbo.DimTerritory dt 
    ON soh.TerritoryID = dt.TerritoryID
LEFT JOIN AdventureDW.dbo.DimDate dd 
    ON CAST(CONVERT(VARCHAR(8), soh.OrderDate, 112) AS INT) = dd.DateKey
LEFT JOIN AdventureWorks2019.Sales.SalesOrderHeaderSalesReason soh_sr
    ON soh.SalesOrderID = soh_sr.SalesOrderID
LEFT JOIN AdventureDW.dbo.DimSalesReason dsr
    ON soh_sr.SalesReasonID = dsr.SalesReasonID;</property>
